<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.AndroidRSSReader" android:versionCode="1"
	android:versionName="1.0">
<uses-permission android:name="android.permission.INTERNET" />
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".AndroidRSSReader" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity android:name=".RssItemDisplayer" android:label="Display Rss Item">
			<intent-filter>
				<action android:name="com.AndroidRSSReader.displayRssItem" />
				<category android:name="android.intent.category.DEFAULT"></category>
			</intent-filter>
		</activity>
		<activity android:name=".AndroidRSSTab1"></activity>
		<activity android:name=".TimetableMain"></activity>
		<activity android:name=".TT_tabs"></activity>
		<activity android:name=".TTList"></activity>
		<activity android:name=".WebShower"></activity>
	</application>

	

</manifest> 